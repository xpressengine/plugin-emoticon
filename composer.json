{
  "name": "xpressengine-plugin/emoticon",
  "description": "에디터에서 이모티콘 기능 제공",
  "keywords": [
    "xpressengine",
    "plugin"
  ],
  "license": "LGPL-3.0-or-later",
  "version": "1.0.0",
  "type": "xpressengine-plugin",
  "support": {
    "email": "developer@xpressengine.com"
  },
  "authors": [
    {
      "name": "XE Developers",
      "email": "developer@xpressengine.com",
      "homepage": "https://store.xehub.io/plugins/emoticon",
      "role": "Developer"
    }
  ],
  "extra": {
    "xpressengine": {
      "title": "editor emoticon",
      "component": {
        "editortool/emoticon@emoticon": {
          "class": "Xpressengine\\Plugins\\Emoticon\\Emoticon",
          "name": "이모티콘",
          "description": "에디터에 이모티콘을 삽입할 수 있도록 해줌"
        }
      }
    }
  },
  "require": {
    "xpressengine/xpressengine": "~3.0.2"
  },
  "autoload": {
    "psr-4": {
      "Xpressengine\\Plugins\\Emoticon\\": "src/"
    }
  }
}
